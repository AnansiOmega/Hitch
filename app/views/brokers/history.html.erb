<div class="logo">
    <%= link_to image_tag("/assets/logo.png"), home_path %><br>
  </div>
    

<h1> Your previous orders for <%= "#{@supplier.company}" %> </h1>

<% @supplier.brokers_history(@broker.id).each do |delivery| %>

